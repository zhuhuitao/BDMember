    <style name="AppTheme.Base" parent="Theme.AppCompat.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="BaseAppTheme" parent="AppTheme.Base">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/theme</item>
        <item name="colorPrimaryDark">@color/theme</item>
        <item name="colorAccent">@color/theme</item>
    </style>

    <style name="BaseAppThemeWhite" parent="AppTheme.Base">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="colorAccent">@color/white</item>
    </style>  <!-- Base application theme. -->


    <style name="AppTheme" parent="BaseAppTheme">
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
    </style>


    <style name="SplashTheme" parent="ImageTranslucentTheme">
        <!-- 欢迎页背景引用刚才写好的 -->
        <!--<item name="android:windowBackground">@mipmap/launchimage</item>-->
        <item name="android:windowFullscreen">false</item>
        <!--透明背景不要了-->
        <item name="android:windowIsTranslucent">false</item>
    </style>


<?xml version="1.0" encoding="utf-8"?>
<com.newdjk.bdmember.widget.FitWindowLayoutV2 xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/sv_activity"
    android:fillViewport="true"
    android:scrollbars="none">

    <RelativeLayout style="@style/match_match">

        <FrameLayout
            android:id="@+id/fl_main"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/include1" />

        <!--底部菜单Bar-->
        <include
            android:id="@+id/include1"
            layout="@layout/layout_bottombars"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="@dimen/dp_1" />

    </RelativeLayout>


</com.newdjk.bdmember.widget.FitWindowLayoutV2>

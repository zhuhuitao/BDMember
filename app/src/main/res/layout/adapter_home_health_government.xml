<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/item_healthGovernment"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_90"
    android:orientation="horizontal"
    android:paddingBottom="@dimen/dp_15">


    <ImageView
        android:id="@+id/iv_home_health_government_them"
        android:layout_width="@dimen/dp_123"
        android:layout_height="match_parent"
        android:scaleType="fitXY"
        android:background="@color/gray" />

    <RelativeLayout
        style="@style/match_match"
        android:layout_marginLeft="@dimen/dp_10">

        <TextView
            android:id="@+id/tv_home_health_government_them"
            style="@style/height_content_width_parent"
            android:text="@string/healthGovernmentThem"
            android:textColor="@color/deep_gray_text"
            android:textSize="@dimen/sp_14" />

        <TextView
            android:id="@+id/tv_home_health_government_time"
            style="@style/height_content_width_parent"
            android:layout_alignParentBottom="true"
            android:text="@string/timeModel"
            android:textColor="@color/shallow_gray_text"
            android:textSize="@dimen/sp_10" />

    </RelativeLayout>


</LinearLayout>
